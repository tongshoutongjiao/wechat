.container{
    position: absolute;
    top: 0;
    bottom: 0;
    width: 100%;
    height: 100%;
}
.text-c{
    text-align: center;
}
.f12{
    font-size: 12px;
}


/*index */
.scanBtn{
    position: absolute;
    bottom:224rpx ;
    right: 328rpx;
    width: 92rpx;
    height: 92rpx;
    background: transparent;
    border-radius: 50%;
}


/* 底部导航栏样式*/

.styleBottom{
    position: absolute;
    bottom: 0rpx;
    height: 106rpx;
    width: 100%;
    background: #fff;
    box-shadow: 0 -2px 5px rgba(205,54,213,0.2);
}
.styleBottom .con{
    position: absolute;
    left: 0;
    top: 0;
    height: 100%;
    width: 33.3%;
    text-align: center;
}
.styleBottom .photoImg{
    left: 0;
}
.styleBottom .photoImg .photoImgIcon{
    width: 42rpx;
    height: 42rpx;
    display: inline-block;
    background-image: url("http://deepfashion.oss-cn-hangzhou.aliyuncs.com/dfshare/photoGray.jpg");
    background-size: cover;
    margin-top: 12rpx;

}
.styleBottom .photoImg{
    line-height: 40rpx;
    font-size: 12px;
    color:#616161;
    letter-spacing:0.22px;
}
.styleBottom .taskIndex{
    left: 33.3%;
}
.styleBottom .taskIndexIconWrapper{
    position: relative;
    display: inline-block;
    width: 106rpx;
    height: 106rpx;
    border-radius: 50%;
    background:#484848;
    padding: 26rpx;
    box-sizing: border-box;
    margin-top: -20rpx;
    box-shadow: 0 -2px 5px rgba(205,54,213,0.2);
    background-image: url("http://deepfashion.oss-cn-hangzhou.aliyuncs.com/dfshare/taskIcon.png");
    background-size: cover;

}
.styleBottom .taskIndexIconWrapper .shadowBox{
    position: absolute;
    width: 100%;
    height: 100%;
    border-radius: 50%;
    left: 0;
    top: 0;
}


.styleBottom .dfSelectWrapper{
    left: 66.6%;
}
.styleBottom .dfSelectWrapper .selectImgIcon{
    display: inline-block;
    width: 46rpx;
    height: 40rpx;
    background-image: url("http://deepfashion.oss-cn-hangzhou.aliyuncs.com/dfshare/unselectIcon.png");
    background-size: cover;
    margin-top: 12rpx;
}
.styleBottom .dfSelectWrapper .textCon{
    line-height: 40rpx;
    font-size: 12px;
    color:#616161;
    letter-spacing:0.22px;
}


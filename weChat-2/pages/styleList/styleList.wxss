.container{
    position: relative;
}
.container .indexBg{

    width: 100%;
    height: 100%;
    background-image: url("http://bi-workbench.oss-cn-hangzhou.aliyuncs.com/wechat_app/indexBg.png");
    background-size: cover;
}
.container .indexBg .content{
    width: 100%;
    font-size: 12px;
    color: #a8a8a8;
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translateX(-50%) translateY(-50%);
}

.styleList{
    position: absolute;
    top: 0;
    bottom: 136rpx;
    width: 100%;
    display: flex;
    flex-flow: row wrap;
    justify-content:space-around;
    align-content: space-around;
}
.style{
    display: inline-block;
    width: 236rpx;
    height: 232rpx;
    background: lightgray;
    border-radius: 8rpx;
    margin: 6rpx;
}
.style image{
    display: inline-block;
    width: 100%;
    height: 100%;
}

.style:nth-child(3n+1){
    margin-left: 10rpx;
}














.container{
    background-image: url(" http://ruhnnstatic.oss-cn-hangzhou.aliyuncs.com/bigbench/indexBg.png");
    background-size: cover;
    position:absolute;
    top:0;
    bottom:0;
    width:100%;
    height: 100%;

}
.partLogo{
    padding-top:70rpx ;
    padding-bottom: 66rpx;
    padding-left: 200rpx;

    width: 100%;

    height:484rpx;
    box-sizing: border-box;
}
.imageWrapper{
    display: inline-block;
    width: 358rpx;
    height: 358rpx;
    padding: 12rpx 26rpx 22rpx 0rpx ;
    box-sizing: border-box;

}
.imageWrapper image{
    display: inline-block;
    height: 314rpx;
    width: 314rpx;
    background: lightblue;
    border-radius: 50%;
}


.logo{
    width: 100%;
}
.teamWrapper{
    width:100%;
    position:absolute;
    top:484rpx;
    bottom:96rpx;
    left:24rpx;
}
.teamWrapper .teamName{
    box-sizing:border-box;
    padding:36rpx 0;
    padding-left:200rpx;
    height:156rpx;
    border-top:1px solid #fff;
    border-bottom:1px solid #DFDFDF;
    margin-right:50rpx;
}
.teamWrapper .teamFirst{
    border-top: 0px;
}
.teamWrapper .teamName text{
    line-height: 100rpx;
    font-size: 12px;

}

/*团队部分*/
.teamName .teamImageIcon{
    display: inline-block;
    width:90rpx ;
    height: 90rpx;
    margin-right: 44rpx;
}
.teamName .teamImageCon{
    display: inline-block;
    vertical-align: top;
}
.teamName .teamImageCon text{
    font-size: 16px;
    line-height: 90rpx;
}
.teamImgText {
    max-width: 350rpx;
    white-space: nowrap;
    text-overflow: ellipsis;
    overflow: hidden;
}

/*底部*/
.bottomFix{
    position:fixed;
    bottom:-10rpx;
    height:117rpx;
    line-height:106rpx;
    color: #fff;
    background:#1D2331;
    width:100%;
    letter-spacing: 12px;
    text-align:center;
    left:0;
}
.createIcon{
    display: inline-block;
    width: 43rpx;
    height:43rpx;
    margin-right: 30rpx;
   margin-top: -5rpx;
    vertical-align: middle;
}
/*图标样式调整
*/
.createIconWrapper{
    display: inline-block;
}
.bottomFix .imgWrapper{
    display: inline-block;
}

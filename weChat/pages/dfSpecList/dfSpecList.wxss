.container{
    padding-left:0 ;
    padding-right: 0;
}
/* 二级标题*/
.container .titleWrapper{
    position: absolute;
    top: 0;
    left: 0rpx;
    right: 0rpx;
    background:#fff;
    height: 94rpx;
    padding-right: 20rpx;
}

.secTitle view{
    height: 100%;
    float: right;

}

/*图片列表区域*/

.container .content{
    position: absolute;
    top: 114rpx;
    bottom: 0;
    overflow: hidden;
    left:0;
    right: 0;
}

/* 遮罩层*/

.layer{
    position:absolute;
    width:92%;
    height:94%;
    background:gray;
    padding:20rpx;
    box-sizing:border-box;
    opacity:0.6;
}

.imgWrapper{
    position: absolute;
    display: inline-block;
    border-radius: 6rpx;
    padding:15rpx ;
    box-sizing: border-box;
}
.imgWrapper .picImage{
    display: inline-block;
    width: 100%;
    height: 100%;
    border-radius: 8rpx;
}
.imgWrapper .activeIcon{
    position: absolute;
    top: 0;
    right: 0;
    bottom: 0;
    left: 0;
    margin: auto;
    width:66rpx ;
    height: 66rpx;
    border-radius: 50%;
    background-image: url("http://bi-workbench.oss-cn-hangzhou.aliyuncs.com/wechat_app/delete1.png");
    background-size: cover;
}
.content .active .activeIcon{
    background-image: url("http://bi-workbench.oss-cn-hangzhou.aliyuncs.com/wechat_app/selected.png");
  z-index: 100;
}


